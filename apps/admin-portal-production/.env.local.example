# Local Development Environment
NEXT_PUBLIC_APP_URL=http://localhost:3001

# Database & Auth
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE=your_supabase_service_role_here

# External Services
GITHUB_TOKEN=your_github_token_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# AI Configuration (Vercel AI SDK)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# AI Workflow Settings (optional)
AI_DEFAULT_MODEL=claude-3-5-sonnet-20241022
AI_MAX_TOKENS=4096
AI_TEMPERATURE=0.3

# Agent Communication (optional)
AGENT_SYSTEM_ID=admin-portal-agent-local
AGENT_ENABLE_LOGGING=true
