                    'DC': { name: 'Documentation', description: 'Technical documentation' },
                    'EG': { name: 'Examples', description: 'Code examples and samples' },
                    'QZ': { name: 'Quiz', description: 'Interactive quizzes' },
                    'FL': { name: 'Flashcards', description: 'Learning flashcards' },
                    'RD': { name: 'Roadmap', description: 'Learning roadmaps' },
                    'PJ': { name: 'Projects', description: 'Hands-on projects' },
                    'CH': { name: 'Challenges', description: 'Coding challenges' },
                    'RE': { name: 'Resources', description: 'Learning resources' },
                    'SU': { name: 'Summary', description: 'Content summarization' },
                    
                    // Support
                    'SO': { name: 'Stack Overflow', description: 'Stack Overflow integration' },
                    'GI': { name: 'GitHub Issues', description: 'GitHub issue tracking' },
                    'BF': { name: 'Bug Fix', description: 'Automated bug fixing' },
                    'ER': { name: 'Error Decoder', description: 'Error analysis and decoding' },
                    'OP': { name: 'Optimization', description: 'Code optimization' },
                    'RE': { name: 'Regex', description: 'Regular expression tools' },
                    'SQ': { name: 'SQL', description: 'SQL query builder' },
                    'AP': { name: 'API', description: 'API development tools' },
                    'JS': { name: 'JSON', description: 'JSON processing tools' },
                    'VA': { name: 'Validation', description: 'Data validation' },
                    
                    // Advanced
                    'GM': { name: 'Game', description: 'Game development' },
                    'AR': { name: 'AR', description: 'Augmented Reality' },
                    'VR': { name: 'VR', description: 'Virtual Reality' },
                    '3D': { name: '3D Viewer', description: '3D model viewer' },
                    'MA': { name: 'Maps', description: 'Interactive maps' },
                    'CH': { name: 'Charts', description: 'Data visualization charts' },
                    'VI': { name: 'Video', description: 'Video processing' },
                    'AU': { name: 'Audio', description: 'Audio processing' },
                    'FI': { name: 'Files', description: 'File management' },
                    'CA': { name: 'Calendar', description: 'Calendar integration' }
                };
            }

            init() {
                this.setupEventListeners();
                this.renderCommandGrid();
                this.startEmailMonitoring();
                this.updateSpendingDisplay();
                this.loadShortcuts();
            }

            setupEventListeners() {
                const commandInput = document.getElementById('commandInput');
                const suggestions = document.getElementById('suggestions');

                commandInput.addEventListener('input', (e) => {
                    this.handleCommandInput(e.target.value);
                });

                commandInput.addEventListener('keydown', (e) => {
                    if (e.key === 'Enter') {
                        this.executeCommand(e.target.value);
                        e.target.value = '';
                        suggestions.style.display = 'none';
                    }
                });

                // Click outside to close suggestions
                document.addEventListener('click', (e) => {
                    if (!commandInput.contains(e.target) && !suggestions.contains(e.target)) {
                        suggestions.style.display = 'none';
                    }
                });
            }

            handleCommandInput(value) {
                const suggestions = document.getElementById('suggestions');
                
                if (value.length < 1) {
                    suggestions.style.display = 'none';
                    return;
                }

                const matches = Object.entries(this.commands)
                    .filter(([code, data]) => 
                        code.toLowerCase().includes(value.toLowerCase()) ||
                        data.name.toLowerCase().includes(value.toLowerCase())
                    )
                    .slice(0, 10);

                if (matches.length > 0) {
                    suggestions.innerHTML = matches.map(([code, data]) => `
                        <div class="suggestion" onclick="godMode.executeCommand('${code}')">
                            <span class="command-code">/${code}</span>
                            <span class="command-desc">${data.name} - ${data.description}</span>
                        </div>
                    `).join('');
                    suggestions.style.display = 'block';
                } else {
                    suggestions.style.display = 'none';
                }
            }

            async executeCommand(command) {
                const cleanCommand = command.replace('/', '').toUpperCase();
                
                if (!this.commands[cleanCommand]) {
                    this.addLogEntry(cleanCommand, 'ERROR', 'Command not found');
                    return;
                }

                // Check spending limits
                if (!this.checkSpendingLimits()) {
                    this.addLogEntry(cleanCommand, 'ERROR', 'Spending limit exceeded');
                    return;
                }

                // Simulate command execution
                this.addLogEntry(cleanCommand, 'SUCCESS', `${this.commands[cleanCommand].name} executed`);
                
                // Update spending
                this.spending.daily += 0.02;
                this.spending.hourly += 0.02;
                this.spending.perRequest = 0.02;
                
                this.updateSpendingDisplay();
                
                // Simulate actual command execution
                await this.simulateCommandExecution(cleanCommand);
            }

            async simulateCommandExecution(command) {
                // Simulate processing time
                await new Promise(resolve => setTimeout(resolve, 1000 + Math.random() * 2000));
                
                // Add success log
                this.addLogEntry(command, 'SUCCESS', `${this.commands[command].name} completed successfully`);
            }

            checkSpendingLimits() {
                return this.spending.daily < this.spending.limits.daily &&
                       this.spending.hourly < this.spending.limits.hourly &&
                       this.spending.perRequest < this.spending.limits.perRequest;
            }

            updateSpendingDisplay() {
                const fill = document.getElementById('spendingFill');
                const percentage = (this.spending.daily / this.spending.limits.daily) * 100;
                fill.style.width = `${Math.min(percentage, 100)}%`;
                
                // Update text
                const spendingText = document.querySelector('.spending-meter').nextElementSibling;
                spendingText.textContent = `$${this.spending.daily.toFixed(2)} / $${this.spending.limits.daily.toFixed(2)} daily limit`;
            }

            addLogEntry(command, status, message) {
                const timestamp = new Date().toLocaleTimeString();
                const logEntry = document.createElement('div');
                logEntry.className = 'log-entry';
                logEntry.innerHTML = `
                    <span class="log-timestamp">[${timestamp}]</span>
                    <span class="log-command">/${command}</span>
                    <span class="log-status log-${status.toLowerCase()}">${status}</span>
                    <span>${message}</span>
                `;
                
                const log = document.getElementById('executionLog');
                log.appendChild(logEntry);
                log.scrollTop = log.scrollHeight;
            }

            renderCommandGrid() {
                const grid = document.getElementById('commandGrid');
                grid.innerHTML = Object.entries(this.commands).map(([code, data]) => `
                    <div class="command-item" onclick="godMode.executeCommand('${code}')">
                        <div class="code">/${code}</div>
                        <div class="name">${data.name}</div>
                        <div class="description">${data.description}</div>
                    </div>
                `).join('');
            }

            startEmailMonitoring() {
                // Simulate email monitoring
                setInterval(() => {
                    this.emailMonitor.lastScan = new Date();
                    this.updateEmailStatus();
                }, 30000); // Update every 30 seconds
            }

            updateEmailStatus() {
                const lastScanElement = document.querySelector('.tag-detection div:last-child');
                if (lastScanElement) {
                    const minutesAgo = Math.floor((Date.now() - this.emailMonitor.lastScan) / 60000);
                    lastScanElement.textContent = `Last scan: ${minutesAgo} minutes ago`;
                }
            }

            loadShortcuts() {
                // Load shortcuts from ~/.cursor-shortcuts
                const shortcuts = localStorage.getItem('cursor-shortcuts');
                if (shortcuts) {
                    try {
                        const parsed = JSON.parse(shortcuts);
                        console.log('Loaded shortcuts:', parsed);
                    } catch (e) {
                        console.error('Error loading shortcuts:', e);
                    }
                }
            }

            saveShortcuts() {
                // Save shortcuts to ~/.cursor-shortcuts
                const shortcuts = {
                    commands: this.commands,
                    spending: this.spending,
                    emailMonitor: this.emailMonitor,
                    timestamp: new Date().toISOString()
                };
                localStorage.setItem('cursor-shortcuts', JSON.stringify(shortcuts));
            }
        }

        // Emergency stop function
        function emergencyStop() {
            if (confirm('Are you sure you want to emergency stop the GODMODE system?')) {
                document.body.classList.add('loading');
                setTimeout(() => {
                    alert('GODMODE system stopped. All processes halted.');
                    location.reload();
                }, 1000);
            }
        }

        // Initialize GODMODE system
        const godMode = new GodModeSystem();

        // Save shortcuts on page unload
        window.addEventListener('beforeunload', () => {
            godMode.saveShortcuts();
        });

        // Auto-save every 5 minutes
        setInterval(() => {
            godMode.saveShortcuts();
        }, 300000);
    </script>
</body>
</html>
